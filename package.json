  "c8y": {
    "application": {
      "name": "Device At Risk Runtime Widget",
      "description": "Custom widget with module federation",
      "contextPath": "device-at-risk-runtime-widget",
      "key": "device-at-risk-runtime-widget-application-key",
      "globalTitle": "Cumulocity Device At Risk Widget with Module Federation",
      "tabsHorizontal": true,
      "isPackage": true,
      "noAppSwitcher": true,
      "package": "plugin",
      "requiredPlatformVersion": ">=1016.0.0",
      "exports": [
        {
          "name": "Device At Risk Runtime Widget",
          "module": "GpDevicesAtRiskWidgetModule",
          "path": "./widget/gp-devices-at-risk-widget.module.ts",
          "description": "Devices at Risk Dashboard - Displays all the devices with risk levels in the dashboard"
